{
  "name": "jp",
  "version": "1.0.0",
  "description": "jest test",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:wath": "jest --watchAll",
    "coverage": "jest --collectCoverage",
    "eslint": "eslint",
    "prettier": "prettier"
  },
  "collectCoverageFrom": [
    "**/*.{js,jsx}",
    "!**/node_modules/**",
    "!**/vendor/**"
  ],
  "coverageThreshold": {
    "branches": 100,
    "functions": 100,
    "lines": 100
  },
  "keywords": [
    "jest",
    "test",
    "jsaqa"
  ],
  "author": "Goryacheva",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.36.1",
    "jest": "^29.6.1",
    "playwright": "^1.36.1"
  }
}
